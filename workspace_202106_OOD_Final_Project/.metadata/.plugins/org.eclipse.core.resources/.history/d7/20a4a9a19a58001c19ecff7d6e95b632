package edu.neu.csye6200;

public class SetVaccination {
public static void Vaccine(String vaccine_Name, int doseNum) {
	Vaccination vaccination;
	switch (vaccine_Name) {
	case  "Hib":{
		vaccination.setHib(doseNum);
	}
	case  "DtaP":{
		Vaccination.setDTaP(doseNum);
	}
	case  "Polio":{
		Vaccination.setPolio(doseNum);
	}
	case  "Hepatitis_B":{
		Vaccination.setHepatitis_B(doseNum);
	}
	case  "MMR":{
		Vaccination.setMMR(doseNum);
	}
	case  "Varicella":{
		Vaccination.setVaricella(doseNum);
	}
	}
}
public void add() {
	
}
}
