package edu.neu.csye6200;

public class VaccinationDemo {
public static void Vaccine(String vaccine_Name, int doseNum) {
	switch (vaccine_Name) {
	case  "Hib":{
		Vaccination.Hib(doseNum);
	}
	case  "DtaP":{
		Vaccination.DTaP(doseNum);
	}
	case  "Polio":{
		Vaccination.Polio(doseNum);
	}
	case  "Hepatitis_B":{
		Vaccination.Hepatitis_B(doseNum);
	}
	case  "MMR":{
		Vaccination.MMR(doseNum);
	}
	case  "Varicella":{
		Vaccination.Varicella(doseNum);
	}
	}
}
public void add() {
	
}
}
