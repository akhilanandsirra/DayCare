package edu.neu.csye6200;

public class VaccinationDemo {
public static void Vaccine(String vaccine_Name) {
	switch (vaccine_Name) {
	case  "Hib":{
		Vaccination.Hib();
	}
	case  "DtaP":{
		Vaccination.DTaP();
	}
	case  "Polio":{
		Vaccination.Polio();
	}
	case  "Hepatitis_B":{
		Vaccination.Hepatitis_B();
	}
	case  "MMR":{
		Vaccination.MMR();
	}
	case  "Varicella":{
		Vaccination.Varicella();
	}
	}
}
}
