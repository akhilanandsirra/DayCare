package edu.neu.csye6200;

public class Student extends Person{

	private int grade;

	public int getGrade() {
		return grade;
	}

	public void setGrade(int grade) {
		this.grade = grade;
	}

	public Student(int age, String name, int grade) {
		super(age, name);
		this.grade = grade;
	}

	@Override
	public String toString() {
		return "Student [grade=" + grade + "]";
	}
	
}
